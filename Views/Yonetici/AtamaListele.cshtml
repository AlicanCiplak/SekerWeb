@using SekerWeb.Models.Entity
@model List<DoktorHasta>
@{
    ViewBag.Title = "AtamaListele";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Hasta</th>
            <th scope="col">Doktor</th>
            <th scope="col">Sil</th>
            <th scope="col">Güncelle</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var atama in Model)
        {
            <tr>

                <td>@atama.Hasta.AdSoyad</td>
                <td>@atama.Hekim.AdSoyad</td>
                <td><a href="#" class="btn btn-danger">Sil </a></td>
                <td><a href="#" class="btn btn-success">Güncelle </a></td>


            </tr>
        }
    </tbody>
</table>

<a href="/Yonetici/DoktorAtama" class="btn btn-primary">Ekle</a>

