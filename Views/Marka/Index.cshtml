@using SekerWeb.Models.Entity
@model List<Marka>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">İD</th>
            <th scope="col">MARKA</th>
            <th scope="col">Sil</th>
            <th scope="col">Güncelle</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var marka in Model)
        {
        <tr>

            <td>@marka.id</td>
            <td>@marka.MarkaAdı</td>
            <td><a href="#" class="btn btn-danger">Sil </a></td>
            <td><a href="#" class="btn btn-success">Güncelle </a></td>


        </tr>
        }
    </tbody>
</table>
<a href="/Marka/MarkaEkle" class="btn btn-primary">Ekle</a>

